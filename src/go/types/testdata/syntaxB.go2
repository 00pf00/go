package p

// any is the empty interface
func _(x any) {
	x = 0
	x = true
	x = "foo"
	x = &x
	x = func(){}
	// etc.
	print(0, true, "foo", &x, func(){})
}

func print(args... any)

// Various options for type parameter syntax so we can play with it.

type _[type P] int
type _[P interface{}] int
type _[P any] int
type _[P,] int
type _[P, Q] int

func _[P](P)
