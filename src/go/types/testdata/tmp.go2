package p

func Reduce(type T1, T2)(s []T1, initializer T2, f func(T2, T1) T2) T2 {
	r := initializer
	for _, v := range s {
		r = f(r, v)
	}
	return r
}

func reducer(float64, int) float64 {
	return 0
}

var _ = Reduce([]int{}, 1.0, reducer)

